{
    "git.autofetch": true,
    "window.zoomLevel": 3,
    "go.formatTool": "goimports",
    "trailing-spaces.trimOnSave": true,
    "go.coverOnSave": true,
    "go.coverOnSingleTest": true,
    "go.delveConfig": {
        "dlvLoadConfig": {
            "followPointers": true,
            "maxVariableRecurse": 1,
            "maxStringLen": 1024,
            "maxArrayValues": 64,
            "maxStructFields": -1
        },
        "apiVersion": 2,
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.fontFamily": "Meslo LG M for Powerline",
    "vs-kubernetes": {
      "vs-kubernetes.minikube-path.mac": "/Users/kcassio/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
      "vscode-kubernetes.minikube-path.mac": "/Users/kcassio/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "workbench.colorTheme": "Abyss",
    "liveshare.allowInvites": "all",
    "editor.tabSize": 2,
    "[go]": {

        "editor.insertSpaces": false,
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "editor.detectIndentation": false,
    "liveshare.authenticationProvider": "GitHub",
}